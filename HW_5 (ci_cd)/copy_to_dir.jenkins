pipeline {
    agent any
    environment {
        FOR_QA_DIR = 'for_qa'
    }
    stages {
        stage('Copy to for_qa Directory') {
            steps {
                script {
                    // copy into for_qa
                    sh "mkdir -p ${env.FOR_QA_DIR}"
                    sh "cp -r * ${env.FOR_QA_DIR}/"
                }
            }
        }
    }
}
